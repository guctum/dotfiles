tap "homebrew/cask"
tap "adoptopenjdk/openjdk"

cask "adoptopenjdk8" # version of java running on servers
cask "adoptopenjdk"  # latest java version
brew "jenv"          # used for switching versions of java
brew "maven"         # needed to build java maven projects
brew "gradle"        # needed to build java gradle projects
brew "wget"          # used to get maven settings.xml from artifactory
